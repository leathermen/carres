<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.nikitades.carres.domain.Car" table="cars">
    <id name="id" type="uuid-char" column="id" />
    <property name="manufacturer" type="text">
      <column name="manufacturer" />
    </property>
    <property name="model" type="text">
      <column name="model" />
    </property>
    <property name="manufacturedAt" type="timestamp">
      <column name="manufactured_at" />
    </property>
    <property name="createdAt" type="timestamp">
      <column name="created_at" />
    </property>
    <property name="updatedAt" type="timestamp">
      <column name="updated_at" />
    </property>
    <property name="createdBy" type="uuid-char">
      <column name="created_by" />
    </property>
    <property name="available" type="boolean">
      <column name="is_available" />
    </property>
  </class>
</hibernate-mapping>